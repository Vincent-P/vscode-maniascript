{
	"Require context": {
		"prefix": "#RequireContext",
		"body": [
			"#RequireContext ${2:Context}",
			"$0"
		]
	},
	"Include": {
		"prefix": "#Include",
		"body": [
			"#Include \"${1:Libs/myLib.Script.txt}\" as ${2:Identifier}",
			"$0"
		]
	},
	"Const": {
		"prefix": "#Const",
		"body": [
			"#Const ${1:Identifier} ${2:value}",
			"$0"
		]
	},
	"Setting": {
		"prefix": "#Setting",
		"body": [
			"#Setting ${1:S_setting} ${2:value} as \"${3}\"",
			"$0"
		]
	},
	"Extends": {
		"prefix": "#Extends",
		"body": [
			"#Extends \"Modes/${1}.Script.txt\"",
			"$0"
		]
	},
	"Declare variable": {
		"prefix": "declare",
		"body": [
			"declare ${1:variable};",
			"$0"
		],
		"description": "Declare a variable"
	},
	"Foreach statement": {
		"prefix": "foreach",
		"body": [
			"foreach (${2:item} in ${3:collection})",
			"{",
			"    $0",
			"}"
		],
		"description": "Foreach statement"
	},
	"For loop": {
		"prefix": "for",
		"body": [
			"for (${1:i}, ${2:start}, ${3:end})",
			"{",
			"    $0",
			"}"
		],
		"description": "for loop"
	},
	"While loop": {
		"prefix": "while",
		"body": [
			"while (${1:True})",
			"{",
			"    $0",
			"}"
		],
		"description": "While loop"
	},
	"If statement": {
		"prefix": "if",
		"body": [
			"if (${1:True})",
			"{",
			"    $0",
			"}"
		],
		"description": "if statement"
	},
	"Else statement": {
		"prefix": "else",
		"body": [
			"else",
			"{",
			"    $0",
			"}"
		],
		"description": "else statement"
	},
	"Switch statement": {
		"prefix": "switch",
		"body": [
			"switch (${1:expr})",
			"{",
			"    $0",
			"    default:",
			"}"
		],
		"description": "Switch statement"
	},
	"Switch type statement": {
		"prefix": "switchtype",
		"body": [
			"switchtype (${1:expr})",
			"{",
			"    $0",
			"    default:",
			"}"
		],
		"description": "Switch type statement"
	}
}